<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="OZE_projekt.CenterPage">
    <MasterDetailPage.Master>
        <ContentPage>


                <ListView x:Name="MenuList">
                    <ListView.ItemTemplate>
                        <DataTemplate>
                            <ImageCell TextColor="Black" DetailColor="Black" Height="25" Text="{Binding MenuTitle}" Detail="{Binding MenuDetail}" ImageSource="{Binding MenuIcon}"/>
                        </DataTemplate>
                    </ListView.ItemTemplate>
                </ListView>

        </ContentPage>
    </MasterDetailPage.Master>
    <MasterDetailPage.Detail>
        <ContentPage>
            <StackLayout>
                                                    <Label> Ja już nie chce porzucam elo  benc </Label>
            </StackLayout>
        </ContentPage>
    </MasterDetailPage.Detail>
</MasterDetailPage>